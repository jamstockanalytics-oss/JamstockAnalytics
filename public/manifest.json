  "shortcuts": [
    {
      "name": "Market Data",
      "short_name": "Market",
      "description": "View live market data",
      "url": "/market",
      "icons": [
        {
          "src": "/icons/shortcut-market.png",
          "sizes": "96x96",
          "type": "image/png"
