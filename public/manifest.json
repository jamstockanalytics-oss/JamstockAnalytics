    {
      "name": "Portfolio",
      "short_name": "Portfolio",
      "description": "Track your portfolio",
      "url": "/portfolio",
      "icons": [
        {
          "src": "/icons/shortcut-portfolio.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
