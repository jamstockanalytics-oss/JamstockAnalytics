{
  "name": "jamstockanalytics",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "deploy": "npx expo export -p web && npx eas-cli@latest deploy",
    "setup-database": "node scripts/setup-database.js",
    "seed-database": "node scripts/seed-database.js",
    "reset-database": "npm run setup-database && npm run seed-database",
    "test-database": "node test-supabase-connection.js",
    "setup-chat-database": "node scripts/setup-chat-database.js",
    "test-chat-integration": "node scripts/test-chat-integration.js",
    "setup-full-database": "npm run setup-database && npm run setup-chat-database && npm run seed-database",
    "test-full-integration": "npm run test-database && npm run test-chat-integration",
    "verify-implementation": "node scripts/verify-implementation.js",
    "test-context-requirements": "node scripts/test-context-requirements.js",
    "setup-rls-policies": "node scripts/setup-rls-policies.js",
    "verify-rls-policies": "node scripts/verify-rls-policies.js",
    "build:auto": "eas build --platform all --profile automated --non-interactive",
    "build:android:auto": "eas build --platform android --profile automated --non-interactive",
    "build:ios:auto": "eas build --platform ios --profile automated --non-interactive",
    "build:web:auto": "npx expo export -p web",
    "build:web:optimized": "npx expo export --platform web --clear",
    "deploy:web": "npm run build:web:optimized",
    "deploy:auto": "npm run build:web:auto && npx eas-cli@latest deploy --non-interactive",
    "setup:auto": "node scripts/auto-setup-env.js && npm run setup-database && npm run seed-database",
    "setup:env": "node scripts/setup-environment.js",
    "build:ci": "npm run setup:auto && npm run build:auto",
    "test:integration:auto": "npm run test-database && npm run test-chat-integration",
    "test:fallback": "node scripts/test-fallback-system.js",
    "setup:ml-agent": "node scripts/setup-ml-agent-simple.js",
    "test:ml-agent": "node scripts/test-ml-agent.js",
    "enable-ai-features": "node scripts/enable-ai-features.js",
    "test-all-ai-features": "node scripts/test-all-ai-features.js",
    "test:environment": "node scripts/test-environment.js",
    "start:web": "npx expo start --web --clear",
    "build:web": "npx expo export --platform web",
    "db:monitor": "node scripts/database-monitoring.js",
    "db:monitor:verbose": "node scripts/database-monitoring.js --verbose",
    "db:monitor:json": "node scripts/database-monitoring.js --json",
    "db:monitor:dry-run": "node scripts/database-monitoring.js --dry-run",
    "db:health-check": "node -e \"const { DatabaseHealthService } = require('./lib/services/database-health'); DatabaseHealthService.getHealthCheckReport().then(console.log).catch(console.error);\"",
    "db:health-check:json": "node -e \"const { DatabaseHealthService } = require('./lib/services/database-health'); DatabaseHealthService.getHealthCheckJSON().then(console.log).catch(console.error);\"",
    "test:db-monitoring": "node scripts/test-database-monitoring.js",
    "db:fix": "echo 'Please run COMPREHENSIVE_DATABASE_FIX_SCRIPT.sql in Supabase SQL Editor'",
    "db:verify": "node scripts/verify-database-fixes.js",
    "db:audit": "echo 'Database audit report: COMPREHENSIVE_DATABASE_AUDIT_REPORT.md'",
    "db:status": "npm run db:verify && npm run db:monitor",
    "release:pro-mode": "node scripts/apply-pro-mode-release.js",
    "release:pro-mode:sql": "echo 'Run scripts/release-pro-mode-public.sql in Supabase SQL Editor'",
    "test:public-access": "node scripts/test-public-access.js",
    "update-database": "node scripts/setup-database.js",
    "db:update": "npm run update-database",
    "db:update:windows": "scripts/update-database.bat",
    "db:update:powershell": "powershell -ExecutionPolicy Bypass -File scripts/update-database.ps1"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "@shopify/flash-list": "2.0.2",
    "@supabase/supabase-js": "^2.75.0",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "deepmerge": "^4.3.1",
    "dotenv": "^17.2.3",
    "expo": "^54.0.1",
    "expo-auth-session": "^7.0.8",
    "expo-constants": "~18.0.9",
    "expo-crypto": "^15.0.7",
    "expo-router": "~6.0.12",
    "expo-web-browser": "^15.0.8",
    "glob": "^11.0.3",
    "lru-cache": "^11.2.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.14.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "rimraf": "^6.0.1",
    "rss-parser": "^3.13.0",
    "zod": "^3.25.76"
  },
  "private": true,
  "devDependencies": {
    "@types/react": "~19.1.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "expo-mcp": "^0.1.13",
    "typescript": "~5.9.2"
  }
}