{
  "name": "jamstockanalytics-production",
  "version": "1.0.0",
  "description": "AI-Powered Jamaica Stock Exchange Market Analysis Platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "webpack --mode production",
    "build:sentry": "SENTRY_RELEASE=jamstockanalytics@1.0.0 webpack --mode production",
    "build:analyze": "ANALYZE_BUNDLE=true webpack --mode production",
    "build:optimize": "NODE_ENV=production webpack --mode production",
    "test": "jest",
    "test:unit": "jest",
    "test:integration": "jest --config jest.integration.config.js",
    "test:all": "jest --config jest.config.js && jest --config jest.integration.config.js",
    "deploy": "npm run build && npm start",
    "perf:audit": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "perf:analyze": "npm run build:analyze",
    "perf:optimize": "node scripts/optimize-performance.js",
    "perf:monitor": "node scripts/performance-monitor.js",
    "perf:budget": "node scripts/optimize-performance.js && node -e \"require('./scripts/optimize-performance.js').validatePerformanceBudget()\"",
    "realtime:test": "node scripts/realtime-test.js",
    "realtime:monitor": "node scripts/realtime-monitor.js",
    "sentiment:test": "node scripts/sentiment-test.js",
    "sentiment:analyze": "node scripts/sentiment-analyze.js",
    "huggingface:test": "node scripts/huggingface-test.js",
    "huggingface:analyze": "node scripts/huggingface-analyze.js",
    "mongodb:setup": "node scripts/mongodb-setup.js",
    "mongodb:test": "node scripts/mongodb-test.js",
    "mongodb:health": "node -e \"require('./services/database').healthCheck().then(console.log).catch(console.error)\"",
    "mongodb:stats": "node -e \"require('./services/database').getDatabaseStats().then(console.log).catch(console.error)\""
  },
  "dependencies": {
    "@sentry/node": "^7.120.4",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.9",
    "puppeteer": "^24.25.0",
    "node-cache": "^5.1.2",
    "redis": "^4.6.12",
    "socket.io": "^4.7.4",
    "stripe": "^14.9.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@sentry/webpack-plugin": "^2.23.1",
    "babel-loader": "^9.1.3",
    "compression-webpack-plugin": "^10.0.0",
    "jest": "^29.7.0",
    "lighthouse": "^11.4.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "jamaica",
    "stock-exchange",
    "jse",
    "ai",
    "market-analysis",
    "financial",
    "investment"
  ],
  "author": "JamStockAnalytics Team",
  "license": "MIT"
}
