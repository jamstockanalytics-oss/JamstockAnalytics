{
  "name": "jamstockanalytics",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "deploy": "npx expo export -p web && npx eas-cli@latest deploy",
    "setup-database": "node scripts/setup-database.js",
    "seed-database": "node scripts/seed-database.js",
    "reset-database": "npm run setup-database && npm run seed-database",
    "test-database": "node test-supabase-connection.js",
    "setup-chat-database": "node scripts/setup-chat-database.js",
    "test-chat-integration": "node scripts/test-chat-integration.js",
    "setup-full-database": "npm run setup-database && npm run setup-chat-database && npm run seed-database",
    "test-full-integration": "npm run test-database && npm run test-chat-integration",
    "verify-implementation": "node scripts/verify-implementation.js",
    "test-context-requirements": "node scripts/test-context-requirements.js",
    "setup-rls-policies": "node scripts/setup-rls-policies.js",
    "verify-rls-policies": "node scripts/verify-rls-policies.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "@shopify/flash-list": "2.0.2",
    "@supabase/supabase-js": "^2.75.0",
    "axios": "^1.12.2",
    "dotenv": "^17.2.3",
    "expo": "^54.0.1",
    "expo-auth-session": "^7.0.8",
    "expo-constants": "~18.0.9",
    "expo-crypto": "^15.0.7",
    "expo-router": "~6.0.12",
    "expo-web-browser": "^15.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.14.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "zod": "^3.25.76"
  },
  "private": true,
  "devDependencies": {
